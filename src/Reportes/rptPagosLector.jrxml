	<subDataset name="conexion2" uuid="b23a3689-95b5-4c47-ac59-d5772b614c28">
		<parameter name="dnilectormonto" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select descripcion, montopag from pago as p inner join prestamo as pre on pre.codigopre = p.codigopre inner join lector as l on l.dnilector = pre.dnilector where l.dnilector = $P{dnilectormonto} and (select extract(month from fechapag)) = (select extract(month from current_date)) and (select extract(year from fechapag)) = (select extract(year from current_date));]]>
		</queryString>
		<field name="descripcion" class="java.lang.String"/>
		<field name="montopag" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="dnilector" class="java.lang.String">
