	<queryString>
		<![CDATA[select p.codigopre, p.fechapre, l.nombre || ' ' || l.apellidos as lector, (current_date - fechapre) as diferencia, ((current_date - fechapre) - 7) as diasmora, to_char(fechapre, 'TMMonth') as mes from prestamo as p inner join lector as l on l.dnilector = p.dnilector where p.estado = true and (current_date - p.fechapre) > 7 and (select extract(year from p.fechapre)) = $P{ano} and (select extract(month from p.fechapre)) = $P{mes};]]>
